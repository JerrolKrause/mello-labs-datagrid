{
    "compilerOptions": {
        "allowUnusedLabels": false,
        "allowUnreachableCode": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strict": true,
        "skipLibCheck": true,

        "target": "es5",
        "baseUrl": "./src",
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "rootDir": "./src",
        "lib": [
            "es2015",
            "dom"
        ],
        "types": []
    },
    "angularCompilerOptions": {
        "fullTemplateTypeCheck": true,
        "preserveWhitespaces": false
    }
}
